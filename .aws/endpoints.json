      "path": "/folder",
      "integration_uri": "arn:aws:lambda:region:account-id:function:folder_service"
    },
    "folder-update": {
      "method": "PUT",
      "path": "/folder/{folderId}",
      "integration_uri": "arn:aws:lambda:region:account-id:function:folder_service"
    },
    "folder-get": {
