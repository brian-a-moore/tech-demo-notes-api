      "path": "/note/{noteId}",
      "integration_uri": "arn:aws:lambda:region:account-id:function:note_service"
    },
    "note-get": {
      "method": "GET",
      "path": "/note/{noteId}",
      "integration_uri": "arn:aws:lambda:region:account-id:function:note_service"
    },
    "note-delete": {
      "method": "DELETE",
